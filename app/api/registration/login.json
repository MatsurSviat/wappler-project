{
  "meta": {
    "options": {
      "linkedFile": "/views/login.ejs",
      "linkedForm": "login1"
    },
    "$_POST": [
      {
        "type": "text",
        "fieldName": "email",
        "name": "email"
      },
      {
        "type": "text",
        "fieldName": "password",
        "name": "password"
      },
      {
        "type": "text",
        "name": "remember"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "identity",
        "module": "auth",
        "action": "login",
        "options": {
          "provider": "wappler",
          "username": "{{$_POST.email}}"
        },
        "output": true,
        "meta": []
      },
      {
        "name": "identity1",
        "module": "auth",
        "action": "identify",
        "options": {
          "provider": "wappler"
        },
        "output": true,
        "meta": []
      }
    ]
  }
}